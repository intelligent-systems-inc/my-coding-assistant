{
    "name": "react-helper-extension",
    "displayName": "React Helper Extension",
    "description": "AI assistant for React best practices, especially useEffect.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:typescript"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
          {
            "command": "react-helper-extension.showSuggestionPanel",
            "title": "Show useEffect Suggestion"
          }
        ],
        "viewsContainers": {
            "activitybar": [
              {
                "id": "reactHelperSidebar",
                "title": "React Helper",
                "icon": "media/icon.png"
              }
            ]
          },
          "views": {
            "reactHelperSidebar": [
              {
                "type": "webview",
                "id": "react-helper-extension.useEffectSuggestionsView",
                "name": "useEffect Suggestions"
              }
            ]
          }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/babel__traverse": "^7.20.7",
        "@types/vscode": "^1.80.0",
        "tslint": "^6.1.3",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@babel/parser": "^7.27.0",
        "@babel/traverse": "^7.27.0",
        "openai": "^4.0.0"
    }
}
